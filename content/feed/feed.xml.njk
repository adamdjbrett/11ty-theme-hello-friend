---
permalink: /feed/feed.xml
layout: null
eleventyExcludeFromCollections: true
---
<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="/feed/pretty-atom-feed.xsl"?>
<feed xmlns="http://www.w3.org/2005/Atom" xml:lang="{{ metadata.language or 'en' }}">
  <title>{{ metadata.title }}</title>
  <subtitle>{{ metadata.feed.subtitle or metadata.subtitle }}</subtitle>
  <link href="{{ metadata.url }}/feed/feed.xml" rel="self" />
  <link href="{{ metadata.url }}/" />
  <updated>{{ collections.postsDesc[0].date | dateISO }}</updated>
  <id>{{ metadata.url }}/</id>
  {% for post in collections.postsDesc %}
  <entry>
    <title>{{ post.data.title }}</title>
    <link href="{{ post.url | absoluteUrl(metadata.url) }}" />
    <id>{{ post.url | absoluteUrl(metadata.url) }}</id>
    <updated>{{ post.date | dateISO }}</updated>
    <summary>{{ post.data.description or post.data.title }}</summary>
  </entry>
  {% endfor %}
</feed>
